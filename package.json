{
  "name": "findsuite",
  "displayName": "FindSuite (Beyond Find)",
  "description": "Ripgrep (aka rg) is a powerful command-line search tool primarily used for finding text in files. Everything is a powerful and lightning-fast desktop search tool. Combining these two programs provides great power and convenience in manipulating files and strings on your computer without leaving vs code.",
  "author": "utocode",
  "publisher": "utocode",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/codesuiteapp/findsuite"
  },
  "license": "GNU General Public License v2.0",
  "keywords": [
    "find",
    "fast find",
    "find jump",
    "ripgrep",
    "grep",
    "rg",
    "fd",
    "super fd",
    "everything",
    "everything find",
    "search",
    "quick search",
    "fuzzy",
    "fuzzy fd",
    "fuzzy find",
    "fuzzy search",
    "fuzzy grep"
  ],
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Language Packs",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Find Suite",
      "properties": {
        "findsuite.compare.external.enabled": {
          "type": "boolean",
          "default": false,
          "description": "%compare.external.enabled.description%"
        },
        "findsuite.compare.external.program": {
          "type": "string",
          "default": "",
          "description": "%compare.external.program.description%"
        },
        "findsuite.compare.external.option": {
          "type": "string",
          "default": "",
          "description": "%compare.external.option.description%"
        },
        "findsuite.fd.internal.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%fd.internal.enabled.description%"
        },
        "findsuite.fd.program.win32": {
          "type": "string",
          "default": "fd.exe",
          "description": "%fd.program.win32.description%"
        },
        "findsuite.fd.program.darwin": {
          "type": "string",
          "default": "fd",
          "description": "%fd.program.darwin.description%"
        },
        "findsuite.fd.program.linux": {
          "type": "string",
          "default": "fd",
          "description": "%fd.program.linux.description%"
        },
        "findsuite.fd.path.win32": {
          "type": "string",
          "default": "",
          "description": "%fd.path.win32.description%"
        },
        "findsuite.fd.path.linux": {
          "type": "string",
          "default": "",
          "description": "%fd.path.linux.description%"
        },
        "findsuite.fd.path.darwin": {
          "type": "string",
          "default": "",
          "description": "%fd.path.darwin.description%"
        },
        "findsuite.fd.defaultOption": {
          "type": "string",
          "default": "-H -s",
          "description": "%fd.default.option.description%"
        },
        "findsuite.fd.excludePatterns": {
          "type": "array",
          "default": [],
          "description": "%fd.exclude.patterns.description%"
        },
        "findsuite.everything.host": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "%everything.server_host.description%"
        },
        "findsuite.everything.port": {
          "type": "integer",
          "default": 3380,
          "description": "%everything.server_port.description%"
        },
        "findsuite.everything.count": {
          "type": "integer",
          "default": 1000,
          "description": "%everything.count.description%"
        },
        "findsuite.everything.limitOpenFile": {
          "type": "integer",
          "default": 100,
          "description": "%everything.limitOpenFile.description%"
        },
        "findsuite.everything.excludePatterns": {
          "type": "array",
          "default": [],
          "description": "%everything.excludePatterns.description%"
        },
        "findsuite.everythingConfig": {
          "type": "object",
          "description": "%everything.search.description%",
          "patternProperties": {
            ".+": {
              "type": "object",
              "required": [
                "enabled",
                "query",
                "regex",
                "fullpath",
                "inWorkspace",
                "description"
              ],
              "properties": {
                "enabled": {
                  "description": "Enabled Search Filter",
                  "type": "boolean"
                },
                "query": {
                  "description": "query",
                  "type": "string"
                },
                "regex": {
                  "description": "regex",
                  "type": "boolean"
                },
                "fullpath": {
                  "description": "fullpath",
                  "type": "boolean"
                },
                "description": {
                  "description": "description",
                  "type": "string"
                },
                "sort": {
                  "description": "sort",
                  "type": "string"
                },
                "ascending": {
                  "description": "ascending",
                  "type": "boolean"
                },
                "inWorkspace": {
                  "description": "inWorkspace",
                  "type": "boolean"
                }
              }
            }
          },
          "default": {
            "filter1": {
              "enabled": true,
              "query": "ext:c;cpp;csv;cxx;docm;dot;dotm;dotx;h;hcl;hpp;htm;html;ini;json;java;lua;mhtml;potm;ppam;ppsm;ppsx;pps;pptm;rtf;sldm;sldx;thmx;txt;vsd;wpd;wri;xlam;xlsb;xlsm;xltm;xltx;xml;yaml;yml",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "ascending": true,
              "description": "Many extension for Text"
            },
            "filter2": {
              "enabled": true,
              "query": "folders:",
              "regex": false,
              "fullpath": true,
              "inWorkspace": false,
              "sort": "path",
              "ascending": false,
              "description": "Full Path [workspace]"
            },
            "filter3": {
              "enabled": true,
              "query": "ext:java;xml;properties;groovy;md;sh;jsp;json;htm;html;yml;yaml;tpl;txt",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "date_modified",
              "ascending": false,
              "description": "Many extension for Java"
            }
          }
        },
        "findsuite.rg.input.preferType": {
          "type": "string",
          "enum": [
            "Input",
            "Incremental"
          ],
          "default": "Input",
          "description": "%rg.input.preferType.description%"
        },
        "findsuite.rg.internal.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%rg.internal.enabled.description%"
        },
        "findsuite.rg.program.win32": {
          "type": "string",
          "default": "rg.exe",
          "description": "%rg.program.win32.description%"
        },
        "findsuite.rg.program.darwin": {
          "type": "string",
          "default": "rg",
          "description": "%rg.program.darwin.description%"
        },
        "findsuite.rg.program.linux": {
          "type": "string",
          "default": "rg",
          "description": "%rg.program.linux.description%"
        },
        "findsuite.rg.count": {
          "type": "integer",
          "default": 500,
          "description": "%rg.count.description%"
        },
        "findsuite.rg.defaultOption": {
          "type": "string",
          "default": "--hidden -S",
          "description": "%rg.default.option.description%"
        },
        "findsuite.rg.matchColor.darkTheme": {
          "type": "string",
          "default": "#ffa500",
          "description": "%rg.matchColor.darkTheme.description%"
        },
        "findsuite.rg.matchColor.lightTheme": {
          "type": "string",
          "default": "#ff1b51",
          "description": "%rg.matchColor.lightTheme.description%"
        },
        "findsuite.rg.custom1": {
          "type": "string",
          "default": "-g \"**/*.java\" -g \"**/.xml\" -g \"**/*.properties\" -g \"**/*.yaml\" -g \"**/*.yml\"",
          "description": "%rg.custom1.description%"
        },
        "findsuite.rg.custom2": {
          "type": "string",
          "default": "",
          "description": "%rg.custom2.description%"
        },
        "findsuite.rg.custom3": {
          "type": "string",
          "default": "@*Mapping",
          "description": "%rg.custom3.description%"
        }
      }
    },
    "menus": {
      "commandPalette": [
        {
          "command": "findsuite.rgWithEverything",
          "when": "isWindows"
        },
        {
          "command": "findsuite.rgWithEverythingFolder",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#folderFiles",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#folder",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#workspace",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#codeWorkspace",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#diff",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything#diffFolder",
          "when": "isWindows"
        },
        {
          "command": "findsuite.incrementalEverything",
          "when": "isWindows"
        },
        {
          "command": "findsuite.everything",
          "when": "isWindows"
        }
      ]
    },
    "commands": [
      {
        "command": "findsuite.fd",
        "title": "Fd multiple files",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fdFile",
        "title": "Fd File",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fdFolder",
        "title": "Open files in Fd Directory",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fdWs",
        "title": "Open files in Fd Workspace",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fd#diff",
        "title": "Diff files in Fd",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fd#diffWs",
        "title": "Diff files in Workspace",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.fd#diffFolder",
        "title": "Diff folder in Fd",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgWithFd",
        "title": "Ripgrep with Fd (Like fd -t f | rg)",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgWithFdDir",
        "title": "Ripgrep with Fd Directory (Like fd -t d | rg)",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rg",
        "title": "Ripgrep",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgFile",
        "title": "Ripgrep in Current File",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgws",
        "title": "Ripgrep in Workspace",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgDirectory",
        "title": "Ripgrep in Directory",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgWithEverything",
        "title": "Ripgrep with Everything (Like everything | rg)",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.rgWithEverythingFolder",
        "title": "Ripgrep with Everything Folder (Like everything | rg)",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.incrementalEverything",
        "title": "Incremental Everything",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything",
        "title": "Everything",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#folder",
        "title": "Open folder thru Everything",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#folderFiles",
        "title": "Open files in Everything Folder",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#workspace",
        "title": "Open files in Everything Workspace",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#codeWorkspace",
        "title": "Everything Workspace from file",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#diff",
        "title": "Everything Diff",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.everything#diffFolder",
        "title": "Everything Diff Folder",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.reveal#top",
        "title": "Reveal to Top",
        "category": "FindSuite"
      },
      {
        "command": "findsuite.reveal#center",
        "title": "Reveal to Center",
        "category": "FindSuite"
      }
    ],
    "keybindings": [
      {
        "command": "findsuite.fdFile",
        "key": "ctrl+alt+f7",
        "mac": "cmd+ctrl+f7"
      },
      {
        "command": "findsuite.fd",
        "key": "ctrl+alt+shift+f7",
        "mac": "cmd+ctrl+shift+f7"
      },
      {
        "command": "findsuite.fdWs",
        "key": "ctrl+alt+9",
        "mac": "cmd+shift+9"
      },
      {
        "command": "findsuite.fd#diff",
        "key": "ctrl+k ctrl+shift+d",
        "mac": "cmd+k cmd+shift+d"
      },
      {
        "command": "findsuite.rgWithFd",
        "key": "ctrl+f7",
        "mac": "cmd+f7"
      },
      {
        "command": "findsuite.rgWithFdDir",
        "key": "ctrl+shift+f7",
        "mac": "cmd+shift+f7"
      },
      {
        "command": "findsuite.rgws",
        "key": "ctrl+alt+f",
        "mac": "cmd+ctrl+f"
      },
      {
        "command": "findsuite.rgFile",
        "key": "ctrl+alt+0",
        "mac": "cmd+shift+0"
      },
      {
        "command": "findsuite.rgWithEverything",
        "key": "ctrl+f10",
        "when": "isWindows"
      },
      {
        "command": "findsuite.rgWithEverythingFolder",
        "key": "ctrl+shift+f10",
        "when": "isWindows"
      },
      {
        "command": "findsuite.incrementalEverything",
        "key": "ctrl+alt+shift+f10",
        "when": "isWindows"
      },
      {
        "command": "findsuite.everything",
        "key": "ctrl+alt+f9",
        "when": "isWindows"
      },
      {
        "command": "findsuite.everything#folderFiles",
        "key": "ctrl+alt+m",
        "when": "isWindows"
      },
      {
        "command": "findsuite.everything#folder",
        "key": "ctrl+alt+4",
        "when": "isWindows"
      },
      {
        "command": "findsuite.everything#codeWorkspace",
        "key": "ctrl+alt+shift+w",
        "when": "isWindows"
      },
      {
        "command": "findsuite.everything#diff",
        "key": "ctrl+k ctrl+alt+d",
        "when": "isWindows"
      },
      {
        "command": "findsuite.reveal#top",
        "key": "ctrl+k ctrl+h"
      },
      {
        "command": "findsuite.reveal#center",
        "key": "ctrl+k ctrl+m"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "vscode:prepublish": "webpack --mode production",
    "compile2": "tsc -p ./",
    "watch2": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/shell-quote": "^1.7.5",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "lodash": "^4.17.21",
    "shell-quote": "^1.8.1"
  }
}

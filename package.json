{
  "name": "findsuite",
  "displayName": "FindSuite",
  "description": "This VS Code extension provides powerful file search capabilities right within your editor. With two main functions, it allows users to quickly find files on their computer using file names, file paths, and regular expressions. Additionally, it leverages ripgrep to efficiently locate files containing specific strings",
  "author": "utocode",
  "publisher": "utocode",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/codesuiteapp/findsuite"
  },
  "license": "GNU General Public License v2.0",
  "keywords": [
    "find",
    "fast find",
    "find jump",
    "ripgrep",
    "grep",
    "rg",
    "everything",
    "everything find",
    "search",
    "quick search",
    "fuzzy",
    "fuzzy find",
    "fuzzy search",
    "fuzzy grep"
  ],
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Language Packs",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "FindSuite",
      "properties": {
        "findsuite.everything.host": {
          "type": "string",
          "default": "127.0.0.1",
          "description": "%everything.server_host.description%"
        },
        "findsuite.everything.port": {
          "type": "integer",
          "default": 3380,
          "description": "%everything.server_port.description%"
        },
        "findsuite.everything.count": {
          "type": "integer",
          "default": 500,
          "description": "%everything.count.description%"
        },
        "findsuite.everything.limitOpenFile": {
          "type": "integer",
          "default": 100,
          "description": "%everything.limitOpenFile.description%"
        },
        "findsuite.everythingConfig": {
          "type": "object",
          "description": "%everything.search.description%",
          "patternProperties": {
            ".+": {
              "type": "object",
              "required": [
                "enabled",
                "query",
                "regex",
                "fullpath",
                "inWorkspace",
                "description"
              ],
              "properties": {
                "enabled": {
                  "description": "Enabled Search Filter",
                  "type": "boolean"
                },
                "query": {
                  "description": "query",
                  "type": "string"
                },
                "regex": {
                  "description": "regex",
                  "type": "boolean"
                },
                "fullpath": {
                  "description": "fullpath",
                  "type": "boolean"
                },
                "description": {
                  "description": "description",
                  "type": "string"
                },
                "sort": {
                  "description": "sort",
                  "type": "string"
                },
                "ascending": {
                  "description": "ascending",
                  "type": "string"
                },
                "inWorkspace": {
                  "description": "inWorkspace",
                  "type": "boolean"
                }
              }
            }
          },
          "default": {
            "defFilter": {
              "enabled": true,
              "query": "",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "description": "Search All"
            },
            "filter1": {
              "enabled": true,
              "query": "ext:c;chm;cpp;csv;cxx;docm;dot;dotm;dotx;h;hpp;htm;html;hxx;ini;java;lua;mht;mhtml;odt;pdf;potx;potm;ppam;ppsm;ppsx;pps;pptm;rtf;sldm;sldx;thmx;txt;vsd;wpd;wri;xlam;xlsb;xlsm;xltm;xltx;xml",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "description": "Many extension for Text"
            },
            "filter2": {
              "enabled": true,
              "query": "folders:",
              "regex": false,
              "fullpath": true,
              "inWorkspace": false,
              "sort": "path",
              "description": "Full Path [workspace]"
            },
            "filter3": {
              "enabled": true,
              "query": "ext:java;xml;properties;groovy;md;sh;jsp;json;htm;html;yml;yaml;tpl;txt",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "description": "Many extension for Java"
            },
            "filter4": {
              "enabled": true,
              "query": "path:c:\\workspace",
              "regex": true,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "description": "Regex"
            },
            "filter5": {
              "enabled": true,
              "query": "",
              "regex": false,
              "fullpath": false,
              "inWorkspace": false,
              "sort": "name",
              "description": "Many extension for Text"
            }
          }
        },
        "findsuite.rg.internal.enabled": {
          "type": "boolean",
          "default": true,
          "description": "%rg.internal.enabled.description%"
        },
        "findsuite.rg.win32.program": {
          "type": "string",
          "default": "rg.exe",
          "description": "%rg.win32.program.description%"
        },
        "findsuite.rg.darwin.program": {
          "type": "string",
          "default": "rg",
          "description": "%rg.darwin.program.description%"
        },
        "findsuite.rg.linux.program": {
          "type": "string",
          "default": "rg",
          "description": "%rg.linux.program.description%"
        },
        "findsuite.rg.count": {
          "type": "integer",
          "default": 300,
          "description": "%rg.count.description%"
        },
        "findsuite.rg.defaultOption": {
          "type": "string",
          "default": "-s",
          "description": "%rg.default.option.description%"
        },
        "findsuite.rg.custom1": {
          "type": "string",
          "default": "-g \"*.java\" -g \"*.xml\" -g \"*.properties\" -g \"*.yaml\" -g \"*.yml\"",
          "description": "%rg.custom1.description%"
        },
        "findsuite.rg.custom2": {
          "type": "string",
          "default": "",
          "description": "%rg.custom2.description%"
        },
        "findsuite.rg.custom3": {
          "type": "string",
          "default": "@*Mapping",
          "description": "%rg.custom3.description%"
        },
        "findsuite.rg.custom4": {
          "type": "string",
          "default": "@*Mapper",
          "description": "%rg.custom4.description%"
        },
        "findsuite.rg.custom5": {
          "type": "string",
          "default": "",
          "description": "%rg.custom5.description%"
        }
      }
    },
    "commands": [
      {
        "command": "utocode.rg",
        "title": "Ripgrep",
        "category": "findsuite"
      },
      {
        "command": "utocode.rgFile",
        "title": "Ripgrep File",
        "category": "findsuite"
      },
      {
        "command": "utocode.rgws",
        "title": "Ripgrep Current Workspace",
        "category": "findsuite"
      },
      {
        "command": "utocode.rgFolder",
        "title": "Ripgrep Folder",
        "category": "findsuite"
      },
      {
        "command": "utocode.rg1",
        "title": "Ripgrep #1",
        "category": "findsuite"
      },
      {
        "command": "utocode.rg2",
        "title": "Ripgrep #2",
        "category": "findsuite"
      },
      {
        "command": "utocode.rg3",
        "title": "Ripgrep #3",
        "category": "findsuite"
      },
      {
        "command": "utocode.rg4",
        "title": "Ripgrep #4",
        "category": "findsuite"
      },
      {
        "command": "utocode.rgThruEverything",
        "title": "Ripgrep with Everything",
        "category": "findsuite"
      },
      {
        "command": "utocode.everything",
        "title": "Everything",
        "category": "findsuite"
      },
      {
        "command": "utocode.everything#folder",
        "title": "Everything Folder",
        "category": "findsuite"
      },
      {
        "command": "utocode.everything#folderFiles",
        "title": "Get All Files after Everything Folder",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter1",
        "title": "Everything #1",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter2",
        "title": "Everything #2",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter3",
        "title": "Everything #3",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter4",
        "title": "Everything #4",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter5",
        "title": "Everything #5",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter6",
        "title": "Everything #6",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter7",
        "title": "Everything #7",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter8",
        "title": "Everything #8",
        "category": "findsuite"
      },
      {
        "command": "utocode.search#filter9",
        "title": "Everything #9",
        "category": "findsuite"
      },
      {
        "command": "utocode.spring.mappings",
        "title": "Spring Mapping",
        "category": "findsuite"
      }
    ],
    "keybindings": [
      {
        "command": "utocode.rgFolder",
        "key": "ctrl+f7",
        "mac": "cmd+f7"
      },
      {
        "command": "utocode.rgws",
        "key": "ctrl+f8",
        "mac": "cmd+f8"
      },
      {
        "command": "utocode.rgFile",
        "key": "ctrl+alt+0",
        "mac": "cmd+option+0"
      },
      {
        "command": "utocode.rg1",
        "key": "ctrl+alt+7",
        "mac": "cmd+option+7"
      },
      {
        "command": "utocode.rg2",
        "key": "ctrl+alt+8",
        "mac": "cmd+option+8"
      },
      {
        "command": "utocode.everything",
        "key": "ctrl+alt+f9",
        "mac": "cmd+option+f9"
      },
      {
        "command": "utocode.everything#folder",
        "key": "ctrl+alt+4",
        "mac": "cmd+option+4"
      },
      {
        "command": "utocode.everything#folderFiles",
        "key": "ctrl+alt+shift+4",
        "mac": "cmd+option+shift+4"
      },
      {
        "command": "utocode.rgThruEverythingWs",
        "key": "ctrl+alt+shift+f10",
        "mac": "cmd+option+shift+f10"
      },
      {
        "command": "utocode.rgThruEverything",
        "key": "ctrl+f10",
        "mac": "cmd+f10"
      },
      {
        "command": "utocode.search#filter1",
        "key": "ctrl+alt+2",
        "mac": "cmd+option+2"
      },
      {
        "command": "utocode.search#filter2",
        "key": "ctrl+alt+3",
        "mac": "cmd+option+3"
      },
      {
        "command": "utocode.spring.mappings",
        "key": "ctrl+alt+m",
        "mac": "cmd+option+m"
      }
    ]
  },
  "scripts": {
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "vscode:prepublish": "webpack --mode production",
    "compile2": "tsc -p ./",
    "watch2": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/shell-quote": "^1.7.5",
    "@types/vscode": "^1.49.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "lodash": "^4.17.21",
    "shell-quote": "^1.8.1"
  }
}